	var getObjDataFromRecords = function(records,selModel){
		return Ext.Array.map(records,function(record){
			var hash = record.getData();
			hash[AgDef.OBJ_URL_DATA_FIELD_ID] += '?'+hash[AgDef.OBJ_TIMESTAMP_DATA_FIELD_ID].getTime();
			delete hash[AgDef.OBJ_TIMESTAMP_DATA_FIELD_ID];
			if(selModel && selModel.isSelected(record)) hash[AgDef.CONCEPT_DATA_COLOR_DATA_FIELD_ID] = self.DEF_SELECTION_RENDERER_COLOR;
			return hash;
		});
	};

