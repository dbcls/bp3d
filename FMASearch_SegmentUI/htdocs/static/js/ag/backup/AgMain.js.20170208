	var getPanelConfig = function(config){

		config = config || {id: Ext.id()};

		var extraParams = {};
		extraParams[AgDef.RELATION_TYPE_NAME] = config.id;

		var store = Ext.create('Ext.data.TreeStore', {
			autoLoad: false,
